<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXMasonryPane?>
<?import de.jensd.fx.fontawesome.Icon?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<StackPane fx:id="root" xmlns:fx="http://javafx.com/fxml/1"
		   style="-fx-background-color: white"
		   xmlns="http://javafx.com/javafx/2.2" fx:controller="quant.test.server.controller.TestPlanController" alignment="CENTER" >
	<children >
		<ScrollPane fitToHeight="true" fitToWidth="true" fx:id="scrollPane" hbarPolicy="NEVER">
			<JFXMasonryPane fx:id="masonryPane" />
		</ScrollPane>

		<JFXButton alignment="BOTTOM_RIGHT" disable="true" StackPane.alignment="BOTTOM_RIGHT" ripplerFill="#4059A9" buttonType="RAISED" style="-fx-background-radius: 32;-fx-background-color: white" fx:id="addTask" AnchorPane.bottomAnchor="24.0"  AnchorPane.rightAnchor="24.0">
			<StackPane.margin>
				<Insets right="16" bottom="16"/>
			</StackPane.margin>
			<graphic>
				<Icon awesomeIcon="PLUS" size="1.6em" styleClass="icon" />
			</graphic>
		</JFXButton>

	</children>

</StackPane>

