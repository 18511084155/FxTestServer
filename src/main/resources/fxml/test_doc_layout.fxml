<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import com.jfoenix.controls.JFXMasonryPane?>

<?import javafx.scene.layout.VBox?>
<?import com.jfoenix.controls.JFXTreeTableView?>
<?import javafx.geometry.Insets?>
<?import com.jfoenix.controls.JFXTreeTableColumn?>
<StackPane fx:id="task_doc" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" >
	<JFXTreeTableView fx:id="treeTableView">

		<VBox.margin>
			<Insets left="12" right="12" top="12" bottom="12"/>
		</VBox.margin>

		<columns>
			<JFXTreeTableColumn fx:id="filePath"
								text="文件名" minWidth="0" prefWidth="120" style="-fx-alignment: center-left; -fx-padding: 0 12 0 12"/>
			<JFXTreeTableColumn fx:id="fileName"
								text="文件路径" minWidth="0" prefWidth="120" style="-fx-alignment: center-left; -fx-padding: 0 12 0 12"/>
			<JFXTreeTableColumn fx:id="createTime"
								text="创建时间" minWidth="0" prefWidth="200" style="-fx-alignment: center-left; -fx-padding: 0 12 0 12"/>
		</columns>
	</JFXTreeTableView>
</StackPane>

